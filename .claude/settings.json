{
  "permissions": {
    "allow": [],
    "deny": ["NotebookRead", "NotebookEdit"],
    "defaultMode": "bypassPermissions"
  },
  "env": {
    "MAX_MCP_OUTPUT_TOKENS": "100000"
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "echo -e '\\a'"
          }
        ]
      }
    ]
  },
  "extraKnownMarketplaces": {
    "goodfoot": {
      "source": {
        "source": "github",
        "repo": "goodfoot-io/marketplace"
      }
    }
  },
  "enabledPlugins": {
    "typescript-hooks@goodfoot": true,
    "typescript-claude-code-for-web-setup@goodfoot": true
  }
}