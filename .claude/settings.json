{
  "env": {
    "MAX_MCP_OUTPUT_TOKENS": "100000",
    "DISABLE_AUTOUPDATER": 1
  },
  "permissions": {
    "allow": [],
    "deny": [
      "NotebookRead",
      "NotebookEdit"
    ],
    "defaultMode": "bypassPermissions"
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "echo -e '\\a'"
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "typescript-hooks@goodfoot": true,
    "typescript-claude-code-for-web-setup@goodfoot": true,
    "project@goodfoot": true,
    "vscode@goodfoot": true,
    "goodfoot@goodfoot": true
  },
  "extraKnownMarketplaces": {
    "goodfoot": {
      "source": {
        "source": "directory",
        "path": "./."
      }
    }
  }
}
